{
  "version": 2,
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "src": "src/*.{js,jsx,ts,tsx}",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}
